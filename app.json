{
  "expo": {
    "name": "নামাজঘড়ি",
    "slug": "namazghori",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "namazghori",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#047857"
    },
    "android": {
      "package": "com.namazghori.app",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#047857"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "VIBRATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.namazghori.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "নামাজঘড়ি-কে সঠিক নামাজের সময় এবং কিবলা দিক নির্ণয়ের জন্য আপনার অবস্থান ব্যবহার করার অনুমতি দিন।"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "supabaseUrl": "https://zsjnypbjlsqcnwxcnnpf.supabase.co",
      "supabaseAnonKey": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Inpzam55cGJqbHNxY253eGNubnBmIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjYwMDcwMjUsImV4cCI6MjA4MTU4MzAyNX0.yeCU_61nYuQAid35kYUFuUv-3PdZGnenpOZUCP0py8Y",
      "eas": {
